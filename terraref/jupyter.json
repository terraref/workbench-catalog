{
    "key": "jupyterterra",
    "label": "Jupyter",
    "description": "Jupyter SciPy Notebook with NetCDF/GDAL dependencies",
    "logo": "/asset/png/logos/jupyter-logo.png",
    "image": {
        "name": "terraref/jupyter-terraref",
        "tags": [
            "latest"
        ]
    },
    "display": "stack",
    "access": "external",
    "authRequired": true,
    "ports": [
        {
            "port": 8888,
            "protocol": "http"
        }
    ],
    "repositories": [
		{
			"type": "git",
			"url" : "https://github.com/jupyter/jupyterlab"
		}
	],
    "volumeMounts": [
        {
            "mountPath": "/home/jovyan/work", "defaultPath": "/"
        }
    ],
    "readinessProbe": {
        "type": "http",
        "path": "/static/base/images/favicon.ico",
        "port": 8888,
        "initialDelay": 5,
        "timeout": 600
    },
    "resourceLimits": {
        "cpuMax": 2000,
        "cpuDefault": 100,
        "memMax": 8000,
        "memDefault": 50
    },
    "info" : "https://nationaldataservice.atlassian.net/wiki/display/NDSC/Jupyter+Notebooks"
}
