{
    "key": "cloud9terra",
    "label": "Cloud9 IDE",
    "image": {
		"name": "terraref/cloud9-terra",
		"tags": ["datadrivenag"]
	}, 
    "display": "stack",
    "access": "external",
    "description": "General-purpose editor with python, gcc, octave support",
    "logo" : "/asset/png/logos/python-logo.png",
    "authRequired": true,
    "ports": [
        {
            "port": 80,
            "protocol": "http",
            "contextPath": "/"
        } ],
    "volumeMounts":[
       { "mountPath": "/workspace", "defaultPath": "/" }
    ],
    "readinessProbe" : {
        "type" : "http",
        "path" : "/favicon.ico",
        "port" : 80,
        "initialDelay": 10,
        "timeout" : 360
    },
    "resourceLimits": {
        "cpuMax": 2000,
        "cpuDefault": 100,
        "memMax": 8000,
        "memDefault": 50
    },
	"info" : "https://github.com/datadrivenag/tutorials/blob/master/workbench.md",
	"tags" : ["7", "20"]
}
