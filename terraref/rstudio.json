{
    "key": "rstudio",
    "label": "RStudio Server",
    "description": "RStudio Server",
    "logo": "/asset/png/logos/rstudio-logo.png",
    "image": {
        "name": "terraref/rstudio-terraref",
        "tags": [
            "latest"
        ]
    },
    "display": "stack",
    "access": "external",
    "ports": [
        {
            "port": 8787,
            "protocol": "http",
            "contextPath": "/"
        } ],
    "repositories": [
        {
            "url": "https://github.com/rstudio/rstudio",
            "type": "git"
        }
    ],
    "readinessProbe": {
        "type": "http",
        "path": "/images/rstudio.png",
        "port": 8787,
        "initialDelay": 1,
        "timeout": 360
    },
    "volumeMounts": [
        {
            "mountPath": "/home/rstudio", "defaultPath" : "/"
        }
    ],
    "resourceLimits": {
        "cpuMax": 2000,
        "cpuDefault": 100,
        "memMax": 8000,
        "memDefault": 50
    },
    "tags": [ "7", "28", "40" ],
	"authRequired": true,
    "info": "https://github.com/terraref/workbench-catalog/blob/datadrivenag/terraref/rstudio.md"
}
